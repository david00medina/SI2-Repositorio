<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.si2.com/destino-turistico#"
     xml:base="http://www.si2.com/destino-turistico"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:destino-turistico="http://www.si2.com/destino-turistico#">
    <owl:Ontology rdf:about="http://www.si2.com/destino-turistico">
        <dc:title>Ontología Destinos Turísticos</dc:title>
        <terms:contributor>Christian Brito Ramos</terms:contributor>
        <terms:contributor>David Alberto Medina Medina</terms:contributor>
        <rdfs:comment xml:lang="es">Ontología de destinos turísticos para la asignatura Sistemas Inteligentes II. 

Universidad de Las Palmas de Gran Canaria.</rdfs:comment>
        <rdfs:label xml:lang="es">Destinos Turisticos</rdfs:label>
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">1.0</owl:versionInfo>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
