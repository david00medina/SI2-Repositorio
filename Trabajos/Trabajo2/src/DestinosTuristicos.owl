    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/elements/1.1/title -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/title"/>
    


    <!-- http://purl.org/dc/terms/contributor -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/contributor"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.si2.com/destino-turistico#contiene -->

    <owl:ObjectProperty rdf:about="http://www.si2.com/destino-turistico#contiene">
        <owl:inverseOf rdf:resource="http://www.si2.com/destino-turistico#perteneceA"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://www.si2.com/destino-turistico#Pais"/>
        <rdfs:range rdf:resource="http://www.si2.com/destino-turistico#Destino"/>
        <rdfs:comment>Los países contienen una serie de posibles destinos turísticos.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.si2.com/destino-turistico#habla -->

    <owl:ObjectProperty rdf:about="http://www.si2.com/destino-turistico#habla">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://www.si2.com/destino-turistico#Turista"/>
        <rdfs:range rdf:resource="http://www.si2.com/destino-turistico#Idioma"/>
        <rdfs:comment>El turista habla un/os idioma/s determinado/s.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.si2.com/destino-turistico#interesadoEn -->

    <owl:ObjectProperty rdf:about="http://www.si2.com/destino-turistico#interesadoEn">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://www.si2.com/destino-turistico#Turista"/>
        <rdfs:range rdf:resource="http://www.si2.com/destino-turistico#Actividad"/>
    </owl:ObjectProperty>
    


    <!-- http://www.si2.com/destino-turistico#perteneceA -->

    <owl:ObjectProperty rdf:about="http://www.si2.com/destino-turistico#perteneceA">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://www.si2.com/destino-turistico#Destino"/>
        <rdfs:range rdf:resource="http://www.si2.com/destino-turistico#Pais"/>
        <rdfs:comment>Un destino pertenece a un país determinado</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.si2.com/destino-turistico#puedeRealizar -->

    <owl:ObjectProperty rdf:about="http://www.si2.com/destino-turistico#puedeRealizar">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://www.si2.com/destino-turistico#Destino"/>
        <rdfs:range rdf:resource="http://www.si2.com/destino-turistico#Actividad"/>
    </owl:ObjectProperty>
    


    <!-- http://www.si2.com/destino-turistico#seAloja -->

    <owl:ObjectProperty rdf:about="http://www.si2.com/destino-turistico#seAloja">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://www.si2.com/destino-turistico#Turista"/>
        <rdfs:range rdf:resource="http://www.si2.com/destino-turistico#Estancia"/>
    </owl:ObjectProperty>
    


    <!-- http://www.si2.com/destino-turistico#seHabla -->

    <owl:ObjectProperty rdf:about="http://www.si2.com/destino-turistico#seHabla">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://www.si2.com/destino-turistico#Pais"/>
        <rdfs:range rdf:resource="http://www.si2.com/destino-turistico#Idioma"/>
        <rdfs:comment>En el país se habla un/os idioma/s determinado/s.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.si2.com/destino-turistico#tiene -->

    <owl:ObjectProperty rdf:about="http://www.si2.com/destino-turistico#tiene">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://www.si2.com/destino-turistico#Destino"/>
        <rdfs:range rdf:resource="http://www.si2.com/destino-turistico#Estancia"/>
    </owl:ObjectProperty>
    


    <!-- http://www.si2.com/destino-turistico#viajaA -->

    <owl:ObjectProperty rdf:about="http://www.si2.com/destino-turistico#viajaA">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://www.si2.com/destino-turistico#Turista"/>
        <rdfs:range rdf:resource="http://www.si2.com/destino-turistico#Pais"/>
        <rdfs:comment>Un turista toma una decisión de viajar a un país.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.si2.com/destino-turistico#Discapacitado -->

    <owl:DatatypeProperty rdf:about="http://www.si2.com/destino-turistico#Discapacitado">
        <rdfs:domain rdf:resource="http://www.si2.com/destino-turistico#Turista"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.si2.com/destino-turistico#NúmeroHijos -->

    <owl:DatatypeProperty rdf:about="http://www.si2.com/destino-turistico#NúmeroHijos">
        <rdfs:domain rdf:resource="http://www.si2.com/destino-turistico#Turista"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#unsignedInt"/>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.si2.com/destino-turistico#Actividad -->

    <owl:Class rdf:about="http://www.si2.com/destino-turistico#Actividad"/>
    


    <!-- http://www.si2.com/destino-turistico#Acuático -->

    <owl:Class rdf:about="http://www.si2.com/destino-turistico#Acuático">
        <rdfs:subClassOf rdf:resource="http://www.si2.com/destino-turistico#Deportivo"/>
    </owl:Class>
    


    <!-- http://www.si2.com/destino-turistico#Aéreo -->

    <owl:Class rdf:about="http://www.si2.com/destino-turistico#Aéreo">
        <rdfs:subClassOf rdf:resource="http://www.si2.com/destino-turistico#Deportivo"/>
    </owl:Class>
    


    <!-- http://www.si2.com/destino-turistico#Camping -->

    <owl:Class rdf:about="http://www.si2.com/destino-turistico#Camping">
        <rdfs:subClassOf rdf:resource="http://www.si2.com/destino-turistico#Estancia"/>
    </owl:Class>
    


    <!-- http://www.si2.com/destino-turistico#Cine -->

    <owl:Class rdf:about="http://www.si2.com/destino-turistico#Cine">
        <rdfs:subClassOf rdf:resource="http://www.si2.com/destino-turistico#Cultural"/>
    </owl:Class>
    


    <!-- http://www.si2.com/destino-turistico#Costero -->

    <owl:Class rdf:about="http://www.si2.com/destino-turistico#Costero">
        <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.si2.com/destino-turistico#puedeRealizar"/>
                        <owl:someValuesFrom rdf:resource="http://www.si2.com/destino-turistico#Acuático"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.si2.com/destino-turistico#puedeRealizar"/>
                        <owl:someValuesFrom rdf:resource="http://www.si2.com/destino-turistico#RelaxPlaya"/>
                    </owl:Restriction>
                </owl:unionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.si2.com/destino-turistico#Destino"/>
    </owl:Class>
    


    <!-- http://www.si2.com/destino-turistico#Cultural -->

    <owl:Class rdf:about="http://www.si2.com/destino-turistico#Cultural">
        <rdfs:subClassOf rdf:resource="http://www.si2.com/destino-turistico#Actividad"/>
    </owl:Class>
    


    <!-- http://www.si2.com/destino-turistico#Deportivo -->

    <owl:Class rdf:about="http://www.si2.com/destino-turistico#Deportivo">
        <rdfs:subClassOf rdf:resource="http://www.si2.com/destino-turistico#Entretenimiento"/>
    </owl:Class>
    


    <!-- http://www.si2.com/destino-turistico#Destino -->

    <owl:Class rdf:about="http://www.si2.com/destino-turistico#Destino">
        <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.si2.com/destino-turistico#puedeRealizar"/>
                        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                        <owl:onClass rdf:resource="http://www.si2.com/destino-turistico#Actividad"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.si2.com/destino-turistico#tiene"/>
                        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                        <owl:onClass rdf:resource="http://www.si2.com/destino-turistico#Estancia"/>
                    </owl:Restriction>
                </owl:unionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.si2.com/destino-turistico#Entretenimiento -->

    <owl:Class rdf:about="http://www.si2.com/destino-turistico#Entretenimiento">
        <rdfs:subClassOf rdf:resource="http://www.si2.com/destino-turistico#Actividad"/>
    </owl:Class>
    


    <!-- http://www.si2.com/destino-turistico#Estancia -->

    <owl:Class rdf:about="http://www.si2.com/destino-turistico#Estancia"/>
    


    <!-- http://www.si2.com/destino-turistico#Exótica -->

    <owl:Class rdf:about="http://www.si2.com/destino-turistico#Exótica">
        <rdfs:subClassOf rdf:resource="http://www.si2.com/destino-turistico#Gastronómica"/>
    </owl:Class>
    


    <!-- http://www.si2.com/destino-turistico#Gastronómica -->

    <owl:Class rdf:about="http://www.si2.com/destino-turistico#Gastronómica">
        <rdfs:subClassOf rdf:resource="http://www.si2.com/destino-turistico#Actividad"/>
    </owl:Class>
    


    <!-- http://www.si2.com/destino-turistico#Hotel -->

    <owl:Class rdf:about="http://www.si2.com/destino-turistico#Hotel">
        <owl:equivalentClass>
            <owl:Class>
                <owl:oneOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.si2.com/destino-turistico#1Estrella"/>
                    <rdf:Description rdf:about="http://www.si2.com/destino-turistico#2Estrellas"/>
                    <rdf:Description rdf:about="http://www.si2.com/destino-turistico#3Estrellas"/>
                    <rdf:Description rdf:about="http://www.si2.com/destino-turistico#4Estrellas"/>
                    <rdf:Description rdf:about="http://www.si2.com/destino-turistico#5Estrellas"/>
                </owl:oneOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.si2.com/destino-turistico#Estancia"/>
    </owl:Class>
    


    <!-- http://www.si2.com/destino-turistico#Idioma -->

    <owl:Class rdf:about="http://www.si2.com/destino-turistico#Idioma"/>
    


    <!-- http://www.si2.com/destino-turistico#IdiomaAmericano -->

    <owl:Class rdf:about="http://www.si2.com/destino-turistico#IdiomaAmericano">
        <owl:equivalentClass>
            <owl:Class>
                <owl:oneOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.si2.com/destino-turistico#EspañolLatino"/>
                    <rdf:Description rdf:about="http://www.si2.com/destino-turistico#FrancésAmericano"/>
                    <rdf:Description rdf:about="http://www.si2.com/destino-turistico#InglésAmericano"/>
                </owl:oneOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.si2.com/destino-turistico#Idioma"/>
    </owl:Class>
    


    <!-- http://www.si2.com/destino-turistico#IdiomaEuropeo -->

    <owl:Class rdf:about="http://www.si2.com/destino-turistico#IdiomaEuropeo">
        <owl:equivalentClass>
            <owl:Class>
                <owl:oneOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.si2.com/destino-turistico#Alemán"/>
                    <rdf:Description rdf:about="http://www.si2.com/destino-turistico#Español"/>
                    <rdf:Description rdf:about="http://www.si2.com/destino-turistico#Francés"/>
                    <rdf:Description rdf:about="http://www.si2.com/destino-turistico#Inglés"/>
                    <rdf:Description rdf:about="http://www.si2.com/destino-turistico#Polaco"/>
                    <rdf:Description rdf:about="http://www.si2.com/destino-turistico#Ruso"/>
                </owl:oneOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.si2.com/destino-turistico#Idioma"/>
    </owl:Class>
    


    <!-- http://www.si2.com/destino-turistico#IdiomaOriental -->

    <owl:Class rdf:about="http://www.si2.com/destino-turistico#IdiomaOriental">
        <owl:equivalentClass>
            <owl:Class>
                <owl:oneOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.si2.com/destino-turistico#Chino"/>
                    <rdf:Description rdf:about="http://www.si2.com/destino-turistico#Hindú"/>
                    <rdf:Description rdf:about="http://www.si2.com/destino-turistico#Japonés"/>
                    <rdf:Description rdf:about="http://www.si2.com/destino-turistico#Tailandés"/>
                </owl:oneOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.si2.com/destino-turistico#Idioma"/>
    </owl:Class>
    


    <!-- http://www.si2.com/destino-turistico#Lúdico -->

    <owl:Class rdf:about="http://www.si2.com/destino-turistico#Lúdico">
        <rdfs:subClassOf rdf:resource="http://www.si2.com/destino-turistico#Entretenimiento"/>
    </owl:Class>
    


    <!-- http://www.si2.com/destino-turistico#Moderna -->

    <owl:Class rdf:about="http://www.si2.com/destino-turistico#Moderna">
        <rdfs:subClassOf rdf:resource="http://www.si2.com/destino-turistico#Gastronómica"/>
    </owl:Class>
    


    <!-- http://www.si2.com/destino-turistico#Museo -->

    <owl:Class rdf:about="http://www.si2.com/destino-turistico#Museo">
        <rdfs:subClassOf rdf:resource="http://www.si2.com/destino-turistico#Cultural"/>
    </owl:Class>
    


    <!-- http://www.si2.com/destino-turistico#Música -->

    <owl:Class rdf:about="http://www.si2.com/destino-turistico#Música">
        <rdfs:subClassOf rdf:resource="http://www.si2.com/destino-turistico#Lúdico"/>
    </owl:Class>
    


    <!-- http://www.si2.com/destino-turistico#Pais -->

    <owl:Class rdf:about="http://www.si2.com/destino-turistico#Pais">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.si2.com/destino-turistico#contiene"/>
                        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                        <owl:onClass rdf:resource="http://www.si2.com/destino-turistico#Destino"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.si2.com/destino-turistico#seHabla"/>
                        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                        <owl:onClass rdf:resource="http://www.si2.com/destino-turistico#Idioma"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.si2.com/destino-turistico#ParqueAtracciones -->

    <owl:Class rdf:about="http://www.si2.com/destino-turistico#ParqueAtracciones">
        <rdfs:subClassOf rdf:resource="http://www.si2.com/destino-turistico#Lúdico"/>
    </owl:Class>
    


    <!-- http://www.si2.com/destino-turistico#PaísAmericano -->

    <owl:Class rdf:about="http://www.si2.com/destino-turistico#PaísAmericano">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.si2.com/destino-turistico#seHabla"/>
                <owl:someValuesFrom rdf:resource="http://www.si2.com/destino-turistico#IdiomaAmericano"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.si2.com/destino-turistico#Pais"/>
    </owl:Class>
    


    <!-- http://www.si2.com/destino-turistico#PaísAsiático -->

    <owl:Class rdf:about="http://www.si2.com/destino-turistico#PaísAsiático">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.si2.com/destino-turistico#seHabla"/>
                <owl:someValuesFrom rdf:resource="http://www.si2.com/destino-turistico#IdiomaOriental"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.si2.com/destino-turistico#Pais"/>
        <owl:disjointWith rdf:resource="http://www.si2.com/destino-turistico#PaísEuropeo"/>
    </owl:Class>
    


    <!-- http://www.si2.com/destino-turistico#PaísEuropeo -->

    <owl:Class rdf:about="http://www.si2.com/destino-turistico#PaísEuropeo">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.si2.com/destino-turistico#seHabla"/>
                <owl:someValuesFrom rdf:resource="http://www.si2.com/destino-turistico#IdiomaEuropeo"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.si2.com/destino-turistico#Pais"/>
    </owl:Class>
    


    <!-- http://www.si2.com/destino-turistico#Privado -->

    <owl:Class rdf:about="http://www.si2.com/destino-turistico#Privado">
        <rdfs:subClassOf rdf:resource="http://www.si2.com/destino-turistico#Estancia"/>
    </owl:Class>
    


    <!-- http://www.si2.com/destino-turistico#PuntosArqueológicos -->

    <owl:Class rdf:about="http://www.si2.com/destino-turistico#PuntosArqueológicos">
        <rdfs:subClassOf rdf:resource="http://www.si2.com/destino-turistico#Cultural"/>
    </owl:Class>
    


    <!-- http://www.si2.com/destino-turistico#Relax -->

    <owl:Class rdf:about="http://www.si2.com/destino-turistico#Relax">
        <rdfs:subClassOf rdf:resource="http://www.si2.com/destino-turistico#Entretenimiento"/>
    </owl:Class>
    


    <!-- http://www.si2.com/destino-turistico#RelaxPlaya -->

    <owl:Class rdf:about="http://www.si2.com/destino-turistico#RelaxPlaya">
        <rdfs:subClassOf rdf:resource="http://www.si2.com/destino-turistico#Relax"/>
    </owl:Class>
    


    <!-- http://www.si2.com/destino-turistico#RelaxRural -->

